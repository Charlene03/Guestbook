<%= render '/shared/title', title: 'Log In'%>
 <hr>
 <div class="well">
   <%= form_tag '/login' do |f| %>
   <div class="form-group">
     <%= label_tag :name %>
     <br>
     <%= text_field_tag :name, params[:name] %>
   </div>
   <div class="form-group">
     <%= label_tag :phone %>
     <br>
     <%= string_field_tag :phone, params[:phone] %>
   </div>
   <br>
   <%= submit_tag "Login", class: "btn btn-success" %>
   <% end %>
 </div> 